{
  // "workbench.colorCustomizations": {
    // "editorError.foreground": "#ff2222",
    // "editorWarning.foreground": "#226622",
    // "editorInfo.foreground": "#662222",
    // "editorHint.foreground": "#222266"
  // },
  "editor.fontFamily": "Hack",
  "editor.fontSize": 15,
  "editor.fontWeight": "bold",
  "editor.minimap.enabled": false,
  "terminal.integrated.fontFamily": "Hack",
  "terminal.integrated.fontSize": 16,
  // "pylint.severity": {
    // "E1101": "Hint",
    // "C0114": "Hint",
    // "C0103": "Hint",
    // "C0116": "Hint"
  // },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.closureStyle": "hide",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "liveServer.settings.donotVerifyTags": true,
  "vim.leader": "<space>",
  "vim.cursorStylePerMode.insert": "block",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block",
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "block",
  "vim.highlightedyank.textColor": "#00FF00",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(0, 255, 0, 0.5)",
  "vim.hlsearch": false,
  "vim.incsearch": true,
  "vim.joinspaces": false,
  "vim.timeout": 1000,
  "vim.useSystemClipboard": true,
  "vim.ignorecase": true,
  "vim.inccommand": "replace",
  "vim.matchpairs": "(:),{:},[:],<:>",
  "vim.showmodename": true,
  "vim.showcmd": true,
  "vim.showMarksInGutter": true,
  "vim.smartcase": true,
  "vim.smartRelativeLine": true,
  "vim.statusBarColorControl": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["z", "t"]
    },
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["B"],
      "after": ["_"]
    },
    {
      "before": ["E"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "*", "p"]
    }
  ],
  "vim.insertModeKeyBindings": [],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["k", "j"],
      "after": ["<ESC>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.visualModeKeyBindings": [],
  "vim.operatorPendingModeKeyBindings": [],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "gitlens.graph.minimap.additionalTypes": [
    "localBranches",
    "stashes",
    "remoteBranches",
    "tags"
  ],
  "zenMode.hideTabs": false,
  "workbench.iconTheme": "material-icon-theme",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.printWidth": 120,
  "files.autoSave": "afterDelay"
}
